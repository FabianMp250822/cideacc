rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      // Permite leer y escribir a cualquier usuario autenticado.
      // 'request.auth != null' significa que el usuario ha iniciado sesi√≥n.
      allow read, write: if request.auth != null;
    }
  }
}
